extends ../layouts/main-layout--tablet-no-config

block title
	title История заказа

block content
	.account
		.account__header
			.account__header-inner
				.account__nav
					ul.account__nav-list
						li.account__nav-item.order-history: a.account__nav-link(href="#") История заказа
						li.account__nav-item.user-profile: a.account__nav-link(href="#") Мой профиль
						li.account__nav-item-partner-program: a.account__nav-link(href="#") Партнерская программа
						li.account__nav-item.account-exit: a.account__nav-link(href="#") Выйти
				a.account-nav__toggle(href="#") Открыть меню
				h1.account__header-title История заказа
		